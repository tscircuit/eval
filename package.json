    "circuit-json-to-connectivity-map": "^0.0.22",
    "circuit-json-to-simple-3d": "^0.0.9",
    "circuit-to-svg": "^0.0.228",
    "comlink": "^4.4.2",
    "concurrently": "^9.1.2",
    "connectivity-map": "^1.0.0",
    "debug": "^4.3.6",
    "flatbush": "^4.5.0",
    "graphics-debug": "^0.0.60",
    "howfat": "^0.3.8",
    "kicad-component-converter": "^0.1.19",
